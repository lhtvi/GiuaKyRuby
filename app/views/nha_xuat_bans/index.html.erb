<p id="notice"><%= notice %></p>

<h1>Nha Xuat Bans</h1>

<table>
  <thead>
    <tr>
      <th>Ma nxb</th>
      <th>Ten nxb</th>
      <th>Dia chi</th>
      <th>Email</th>
      <th>Thong tin nguoi dai dien</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nha_xuat_bans.each do |nha_xuat_ban| %>
      <tr>
        <td><%= nha_xuat_ban.ma_nxb %></td>
        <td><%= nha_xuat_ban.ten_nxb %></td>
        <td><%= nha_xuat_ban.dia_chi %></td>
        <td><%= nha_xuat_ban.email %></td>
        <td><%= nha_xuat_ban.thong_tin_nguoi_dai_dien %></td>
        <td><%= link_to 'Show', nha_xuat_ban %></td>
        <td><%= link_to 'Edit', edit_nha_xuat_ban_path(nha_xuat_ban) %></td>
        <td><%= link_to 'Destroy', nha_xuat_ban, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nha Xuat Ban', new_nha_xuat_ban_path %>
